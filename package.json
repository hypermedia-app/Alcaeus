{
  "name": "alcaeus",
  "version": "0.7.0",
  "description": "Hydra Core hypermedia-aware client library",
  "main": "lib/node/index.js",
  "types": "types/index.d.ts",
  "module": "lib/es/index.js",
  "module.root": "lib/es",
  "directories": {
    "test": "tests"
  },
  "files": [
    "types",
    "lib"
  ],
  "scripts": {
    "test": "jest",
    "build": "npm run build:nodejs; npm run build:default",
    "build:nodejs": "tsc --target es2017 --module commonjs --outDir lib/node",
    "build:default": "tsc --target esnext --module esnext --outDir lib/es",
    "build:gitbook": "gitbook install; gitbook build . docs/latest",
    "lint": "eslint . --ext .ts --quiet",
    "prepare": "rm -rf lib; npm run build",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/wikibus/alcaeus"
  },
  "keywords": [
    "ajax",
    "hydra",
    "semanticweb",
    "hypermedia",
    "rdf",
    "json-ld"
  ],
  "author": "Tomasz Pluskiewicz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wikibus/alcaeus/issues"
  },
  "homepage": "https://github.com/wikibus/alcaeus#readme",
  "dependencies": {
    "@rdfjs/parser-jsonld": "^1.1.1",
    "@rdfjs/serializer-jsonld": "^1.2.0",
    "core-decorators": "^0.20.0",
    "es6-url-template": "^1.0.3",
    "isomorphic-fetch": "^2.2.1",
    "jsonld": "^1.4.0",
    "parse-link-header": "^1.0.1",
    "rdf-ext": "^1.0.0",
    "string-to-stream": "^1.1.0",
    "tsmonad": "^0.8.0"
  },
  "devDependencies": {
    "@rdfjs/parser-n3": "^1.1.2",
    "@types/core-decorators": "^0.10.32",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jasmine": "^2.5.53",
    "@types/sinon": "^7.0.13",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es3": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "core-js": "^2.5.0",
    "gitbook-cli": "^2.3.2",
    "istanbul-instrumenter-loader": "^3.0.1",
    "json-server": "^0.12.1",
    "karma": "^4.1.0",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-summary-reporter": "^1.5.0",
    "karma-webpack": "^3.0.5",
    "sinon": "^7.3.2",
    "ts-loader": "^2.3.3",
    "eslint": "^6.0.1",
    "@typescript-eslint/parser": "^1.11.0",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "standard": "^12.0.1",
    "typescript": "^3.1.0-dev.20180721",
    "webpack": "^3.5.5",
    "webpack-karma-jasmine": "^1.0.4",
    "whatwg-fetch": "^3.0.0",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.0",
    "standard-version": "^6.0.1",
    "husky": "^1.0.0",
    "lint-staged": "^8.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "@types/jest": "^24.0.16",
    "eslint-plugin-jest": "^22.14.1"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
