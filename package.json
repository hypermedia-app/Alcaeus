{
  "name": "alcaeus",
  "version": "1.0.0-alpha.8",
  "description": "Hydra Core hypermedia-aware client library",
  "main": "lib/node/index.node.js",
  "types": "types/index.d.ts",
  "module": "lib/es/index.js",
  "module.root": "lib/es",
  "directories": {
    "test": "tests"
  },
  "files": [
    "types",
    "lib"
  ],
  "scripts": {
    "test": "jest",
    "build": "npm run build:nodejs; npm run build:default",
    "build:nodejs": "tsc -p tsconfig.node.json",
    "build:default": "tsc -p tsconfig.es.json",
    "build:gitbook": "gitbook install; gitbook build . docs/latest",
    "gitbook": "gitbook serve",
    "lint": "eslint . --ext .ts --quiet",
    "prepack": "rm -rf lib; npm run build",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/wikibus/alcaeus"
  },
  "keywords": [
    "ajax",
    "hydra",
    "semanticweb",
    "hypermedia",
    "rdf",
    "json-ld"
  ],
  "author": "Tomasz Pluskiewicz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wikibus/alcaeus/issues"
  },
  "homepage": "https://github.com/wikibus/alcaeus#readme",
  "dependencies": {
    "@rdfjs/sink-map": "^1.0.1",
    "@tpluscode/rdfine": "^0.2.9",
    "es6-url-template": "^1.0.3",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-form-data": "^2.0.0",
    "parse-link-header": "^1.0.1",
    "rdf-ext": "^1.0.0",
    "rdf-transform-triple-to-quad": "^1.0.1",
    "string-to-stream": "^3.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.0",
    "@rdfjs/parser-jsonld": "^1.2.0",
    "@rdfjs/parser-n3": "^1.1.3",
    "@types/clownface": "^0.12.4",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^24.0.17",
    "@types/rdf-ext": "^1.3.3",
    "@types/rdf-js": "^2.0.8",
    "@types/rdfjs__namespace": "^1.1.1",
    "@types/rdfjs__parser-jsonld": "^1.2.2",
    "@types/rdfjs__parser-n3": "^1.1.2",
    "@types/rdfjs__sink-map": "^1.0.0",
    "@types/sinon": "^7.0.13",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "@zazuko/rdf-vocabularies": "^2019.10.22",
    "core-js": "^2.5.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^6.0.1",
    "eslint-plugin-jest": "^22.14.1",
    "gitbook-cli": "^2.3.2",
    "gitbook-plugin-advanced-emoji": "^0.2.2",
    "gitbook-plugin-etoc": "^1.2.2",
    "gitbook-plugin-forkmegithub": "^2.2.0",
    "gitbook-plugin-hints": "^1.0.2",
    "gitbook-plugin-runkit": "0.0.1",
    "husky": "^1.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jest": "^24.8.0",
    "json-server": "^0.12.1",
    "jsonld": "^1.4.0",
    "lint-staged": "^8.0.0",
    "sinon": "^7.3.2",
    "standard": "^12.0.1",
    "standard-version": "^7.0.1",
    "typescript": "^3.7.4",
    "whatwg-fetch": "^3.0.0"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --quiet --fix --ignore-path .eslintignore",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
